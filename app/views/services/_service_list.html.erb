<div class="row mb-3">
  <div class="col">
    <div class="card">
      <div class="card-body">
        <% if @client.services.present? %>
          <%= render 'services/scheduled_service', service: @client.services.last %>
          <%= render 'services/service_history' if @client.services.length > 1 %>
        <% else %>
          <h5 class="text-muted mb-0"><%= t '.no_service' %></h5>
        <% end %>
      </div>
    </div>
  </div>
</div>